<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane id="AnchorPane" prefHeight="768.0" prefWidth="1024.0" stylesheets="@../Resource/MyTheme.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.excellentsystem.sentralindahperdana.View.DataBarangController">
   <children>
      <GridPane hgap="5.0" vgap="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="searchField" maxWidth="300.0" prefHeight="30.0" prefWidth="300.0" promptText="Search..." styleClass="search-field" GridPane.halignment="RIGHT" />
            <TableView fx:id="barangTable" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="true" GridPane.rowIndex="1">
              <columns>
                <TableColumn fx:id="kodeKategoriColumn" prefWidth="75.0" text="Kode Kategori" />
                <TableColumn fx:id="kodeBarangColumn" prefWidth="75.0" text="Kode Barang" />
                  <TableColumn fx:id="namaBarangColumn" prefWidth="75.0" text="Nama Barang" />
                  <TableColumn fx:id="keteranganColumn" prefWidth="75.0" text="Keterangan" />
                  <TableColumn fx:id="satuanDasarColumn" prefWidth="75.0" text="Satuan Dasar" />
                  <TableColumn fx:id="labaPersenColumn" prefWidth="75.0" text="Laba Persen" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </GridPane>
   </children>
</AnchorPane>
